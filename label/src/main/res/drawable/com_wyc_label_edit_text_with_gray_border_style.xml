<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <!-- 形状颜色 -->
            <solid android:color="@color/com_wyc_label_white"
                />
            <!--边框颜色-->
            <stroke
                android:width="@dimen/com_wyc_label_size_1"
                android:color="@color/com_wyc_label_gray_sub_transparent" />
            <!--圆角-->
            <corners android:radius="@dimen/com_wyc_label_size_2"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/com_wyc_label_disabled_gray" />
            <stroke android:width="0dp"/>
            <corners android:radius="@dimen/com_wyc_label_size_2"/>
        </shape>
    </item>
</selector>