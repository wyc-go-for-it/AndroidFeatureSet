<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopDrawableTextView">
        <attr name="verSpacing" format="dimension" />
        <attr name="bottomSpacing" format="dimension" />
        <attr name="animType" format="integer"/>
        <attr name="selectTextColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="FlowLayout_Layout">
        <attr name="layout_weight_width" format="float"/>
        <attr name="layout_weight_height" format="float"/>
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="item_horizontal_spacing" format="dimension"/>
        <attr name="item_vertical_spacing" format="dimension"/>
        <attr name="separator_color" format="color"/>
        <attr name="separator_size" format="dimension"/>
        <attr name="closing" format="boolean"/>
        <attr name="rowCount" format="integer"/>
        <attr name="hasTitle" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ItemPaddingLinearLayout">
        <attr name="ItemPadding" format="dimension"/>
        <attr name="PaddingColor" format="color"/>
    </declare-styleable>
</resources>